alias -g common.txt=/usr/share/seclists/Discovery/Web-Content/common.txt
alias -g quickhits.txt=/usr/share/seclists/Discovery/Web-Content/quickhits.txt
alias nst="sudo nmap -v -T4 -Pn -A"
alias nsa="sudo nmap -v -T4 -Pn -A -p-"
alias nsaf="sudo nmap -v -T4 -Pn -A -p- --defeat-rst-ratelimit"
alias nu="sudo nmap -v -T4 -Pn -sU -sV --version-intensity=0"
alias ffuf="ffuf -H 'User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/117.0.5938.132 Safari/537.36' -ic"
alias wpscan="wpscan --rua"
